@model List<Bug_Tracker.Models.UserAdminModel>
@{
    ViewBag.Title = "User Admin";
}
<h2>@ViewBag.Title.</h2>
<p>Use this area to provide additional information.</p>

<table class="table table-hover">
    <tr>
        <th>UserName</th>
        <th>Email</th>
        <th>Role</th>
    </tr>
    @foreach (var u in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => u.UserName)</td>
            <td>@Html.DisplayFor(modelItem => u.Email)</td>
            <td>@Html.DisplayFor(modelItem => u.Role)</td>
            <td>@Html.ActionLink("View", "ViewUser", "UserAdmin")</td>
        </tr>
    }
</table>